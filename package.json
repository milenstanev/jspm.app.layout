{
  "name": "milenstanev-jspm.angular.template",
  "version": "0.0.1",
  "description": "",
  "registry": "github",
  "main": "dist/index.dist.js",
  "scripts": {
    "start": "npm install && npm run postinstall && npm run server",
    "server": "python -m SimpleHTTPServer 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "./node_modules/.bin/jspm install -y",
    "jspm-link": "./node_modules/.bin/jspm link -y",
    "jspm-update": "./node_modules/.bin/jspm update && ./node_modules/.bin/jspm clean",
    "jspm-unbundle": "./node_modules/.bin/jspm unbundle",
    "jspm-bundle": "./node_modules/.bin/jspm bundle src/index.js dist/index.bundle.js --inject --minify --no-mangle"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jspm": "^0.16.51"
  },
  "jspm": {
    "dependencies": {
      "angular": "angular/bower-angular@^1.6.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "eslint": "^3.18.0",
    "gulp": "^3.9.1",
    "jspm-git": "^0.6.0",
    "less": "^2.7.2",
    "systemjs-builder": "^0.16.0"
  }
}
